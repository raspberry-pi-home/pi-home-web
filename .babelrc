{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions",
                    ],
                    "node": "current",
                },
                "modules": false,
                "loose": true,
            },
        ],
        "stage-3",
        "react",
    ],
    "plugins": [
        "transform-class-properties",
        "dynamic-import-webpack",
    ],
    "env": {
        "production": {
            "plugins": [
                "react-remove-properties",
            ],
        },
        "test": {
            "presets": [
                "env",
                "stage-3",
                "react",
            ],
            "plugins": [
                "dynamic-import-node",
                "transform-class-properties",
            ],
        },
    },
}
